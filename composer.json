{
    "name": "glasswalllab/wiise_connector",
    "description": "Connection to Wiise (Microsoft Business Central)",
    "keywords": [
        "glasswalllab",
        "wiise-connector"
    ],
    "homepage": "https://github.com/glasswalllab/wiise-connector",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Stephen Reid",
            "email": "sreid@gwlab.com.au",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0",
        "league/oauth2-client": "^2.6"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Glasswalllab\\Wiise_connector\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Glasswalllab\\Wiise_connector\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Glasswalllab\\Wiise_connector\\WiiseConnectorServiceProvider"
            ],
            "aliases": {
                "WiiseConnector": "Glasswalllab\\Wiise_connector\\WiiseConnectorFacade"
            }
        }
    }
}
